<esa/18351aee-9dbd-4db2-a1d8-ba5eb857569e> a skos:Concept ;
    skos:broader <esa/bd342f1a-d481-4710-80c5-aea3f7078c89> ;
    skos:inScheme <> ;
    skos:notation "4.3.1" ;
