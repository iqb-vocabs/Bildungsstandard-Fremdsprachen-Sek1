        "https://example.com/iqb/bista2004ef1/msae/4/4.4",
        "https://example.com/iqb/bista2004ef1/msae/4/4.5",
        "https://example.com/iqb/bista2004ef1/msae/4/4.6" .

<msae/4/4.1> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4" ;
    skos:prefLabel "Textrezeption (Leseverstehen und Hörverstehen)" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msae/4/4.1/4.1.1",
        "https://example.com/iqb/bista2004ef1/msae/4/4.1/4.1.2",
        "https://example.com/iqb/bista2004ef1/msae/4/4.1/4.1.3",
        "https://example.com/iqb/bista2004ef1/msae/4/4.1/4.1.4" .

<msae/4/4.1/4.1.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.1" ;
    skos:prefLabel "verschiedene Hör- und Lesetechniken auf unterschiedliche Textarten (z. B. Sachtexte, Artikel, literarische Kleinformen) anwenden," .

<msae/4/4.1/4.1.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.1" ;
    skos:prefLabel "sich schnell einen groben Überblick über den Inhalt eines Textes verschaffen," .

<msae/4/4.1/4.1.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.1" ;
    skos:prefLabel "wichtige Details durch Unterstreichen markieren," .

<msae/4/4.1/4.1.4> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.1" ;
    skos:prefLabel "wichtige Textstellen durch farbliches Hervorheben, durch das Notieren von Stichworten und durch ordnende ergänzende Randnotizen besonders kenntlich machen." .

<msae/4/4.2> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4" ;
    skos:prefLabel "Interaktion" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msae/4/4.2/4.2.1",
        "https://example.com/iqb/bista2004ef1/msae/4/4.2/4.2.2",
        "https://example.com/iqb/bista2004ef1/msae/4/4.2/4.2.3" .

<msae/4/4.2/4.2.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.2" ;
    skos:prefLabel "sich in der Klasse, mit Partnern und in Gruppen in der Fremdsprache verständigen und Kommunikationsprozesse aufrecht erhalten," .

<msae/4/4.2/4.2.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.2" ;
    skos:prefLabel "sich in realen Alltagssituationen mit unterschiedlichen, englischsprachigen Personen verständigen und Verständigungsprobleme durch Rückfragen, durch Vereinfachungen, durch Höflichkeitsformeln und nonverbale Mittel überwinden," .

<msae/4/4.2/4.2.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.2" ;
    skos:prefLabel "Techniken des Vermittelns zwischen zwei Sprachen einsetzen." .

<msae/4/4.3> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4" ;
    skos:prefLabel "Textproduktion (Sprechen und Schreiben)" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msae/4/4.3/4.3.1",
        "https://example.com/iqb/bista2004ef1/msae/4/4.3/4.3.2",
        "https://example.com/iqb/bista2004ef1/msae/4/4.3/4.3.3",
        "https://example.com/iqb/bista2004ef1/msae/4/4.3/4.3.4" .

<msae/4/4.3/4.3.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.3" ;
    skos:prefLabel "sich Informationen aus unterschiedlichen fremdsprachlichen Textquellen beschaffen, die se vergleichen, auswählen und bearbeiten," .

<msae/4/4.3/4.3.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.3" ;
    skos:prefLabel "Techniken des Notierens zur Vorbereitung eigener Texte oder Präsentationen nutzen," .

<msae/4/4.3/4.3.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.3" ;
    skos:prefLabel "mit Hilfe von Stichworten, Gliederungen, Handlungsgeländern Texte mündlich vortragen oder schriftlich verfassen," .

<msae/4/4.3/4.3.4> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4/4.3" ;
    skos:prefLabel "die Phasen des Schreibprozesses (Entwerfen, Schreiben, Überarbeiten) selbstständig durchführen." .

<msae/4/4.4> skos:definition "Lernstrategien beziehen sich auf sprachliche Mittel, auf Aussprache, Intonation, Orthographie sowie auf Grammatik. Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msae/4" ;
    skos:prefLabel "Lernstrategien" ;
