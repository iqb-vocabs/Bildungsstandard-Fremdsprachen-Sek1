    skos:inScheme <> ;
    skos:notation "4.2.3" ;
    skos:prefLabel "einfache Techniken des Vermittelns zwischen zwei Sprachen einsetzen."@de .

<esa/f3587e3e-8432-4a96-b3f0-4adae2179529> a skos:Concept ;
    skos:broader <esa/9d257e78-4e3b-4a45-8f6c-951fe04a80ac> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "sind neugierig auf Fremdes, aufgeschlossen für andere Kulturen und akzeptieren kulturelle Vielfalt ohne Angst und Vorbehalte,"@de .

<esa/f495708a-fc19-4ae0-9ba3-9bb83f165078> a skos:Concept ;
    skos:broader <esa/4abc6f4b-50e6-4344-b1ad-d1fc23d773d7> ;
    skos:inScheme <> ;
    skos:notation "1.3.6" ;
    skos:prefLabel "sich in einfachen Routinesituationen (Einkaufen, Essen, öffentliche Verkehrsmittel) verständigen und Informationen geben und erfragen (A2),"@de .

<esa/f5559349-f9a0-431c-9766-7c16c5781752> a skos:Concept ;
    skos:broader <esa/e88310f4-1420-4e18-a73b-5266ac16b1cb> ;
    skos:inScheme <> ;
    skos:notation "2.3.2" ;
    skos:prefLabel "nutzen Zeichen der Lautschrift als Aussprachehilfe,"@de .

<esa/f789b0b6-c653-42ca-9af2-3098cb696e2f> a skos:Concept ;
    skos:broader <esa/5fc7d558-ed90-45ce-bc2a-adececf5b4df> ;
    skos:definition "Die Schülerinnen und Schüler können mündlich einfache sprachliche Äußerungen von der einen in die andere Sprache sinngemäß übertragen."@de ;
    skos:inScheme <> ;
    skos:notation "1.6" ;
    skos:prefLabel "Sprachmittlung"@de .

<esa/fe59b623-d685-4c42-a7eb-2a0d64326f8b> a skos:Concept ;
    skos:broader <esa/d9feea65-35b5-4bbd-9d29-a474694b1e24> ;
    skos:inScheme <> ;
    skos:notation "3.2.4" ;
    skos:prefLabel "Werten, Normen, Überzeugungen, Einstellungen (u.a. in Bezug auf regionale Kulturen, Traditionen, Geschichte, Minderheiten, Kunst)."@de .

<esa/ff76060f-c939-4f97-8e87-8491b17183b7> a skos:Concept ;
    skos:broader <esa/a20df2fc-038b-44c5-9973-6501fdf61a6f> ;
    skos:inScheme <> ;
    skos:notation "1.4.3" ;
    skos:prefLabel "kurz und einfach über eine Tätigkeit oder ein Ereignis berichten (A2)."@de .

<msae/054fbf97-7cf8-4cdc-acf1-d25b504e8f1b> a skos:Concept ;
    skos:broader <msae/9e02c338-d884-4787-9690-0cc45a194948> ;
    skos:inScheme <> ;
    skos:notation "1.3.7" ;
    skos:prefLabel "in Gesprächen und Diskussionen kurz zu den Standpunkten anderer Stellung nehmen und höflich Überzeugungen und Meinungen, Zustimmung und Ablehnung ausdrücken (B1/ B1+)."@de .

<msae/0d87b3d0-930a-4977-903e-4ccaeaf6e0b9> a skos:Concept ;
    skos:broader <msae/0bf48a0c-1c82-45f7-ad68-91ff97e53eb2> ;
    skos:inScheme <> ;
    skos:notation "1.6.2" ;
    skos:prefLabel "persönliche und einfache Sach- und Gebrauchstexte sinngemäß übertragen."@de .

<msae/0dd1e5c3-2ea1-4688-aae5-e627b0912101> a skos:Concept ;
    skos:broader <msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe> ;
    skos:inScheme <> ;
    skos:notation "3.1.6" ;
    skos:prefLabel "kennen gängige Sicht- und Wahrnehmungsweisen, Vorurteile und Stereotype des eigenen und des fremdkulturellen Landes und setzen sich mit ihnen auseinander,"@de .

<msae/1115d485-6d2c-4046-8e0b-8c7c80b0fc5b> a skos:Concept ;
    skos:broader <msae/395c1569-a353-4cd5-bd95-3567528dc4a5> ;
    skos:inScheme <> ;
    skos:notation "1.1.2" ;
    skos:prefLabel "Vorträge verstehen, wenn die Thematik vertraut und die Darstellung unkompliziert und klar strukturiert ist (B1+),"@de .

<msae/1503de73-c0a0-4eb7-9135-1e9591515504> a skos:Concept ;
    skos:broader <msae/f3a0d994-06d4-48a1-bf71-356addf2ea3a> ;
    skos:definition "Die Schülerinnen und Schüler können unter Anwendung der Rechtschreib- und Zeichensetzungsregeln der Zielsprache ausreichend korrekt und verständlich schreiben."@de ;
    skos:inScheme <> ;
    skos:notation "2.4" ;
    skos:prefLabel "Orthographie"@de .

<msae/1aebe83d-b949-44a6-93b3-d4f8a3af0cc4> a skos:Concept ;
    skos:broader <msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe> ;
    skos:inScheme <> ;
    skos:notation "3.1.3" ;
    skos:prefLabel "sind bereit, sich auf fremde Situationen einzustellen und sich in Situationen des Alltagslebens angemessen zu verhalten,"@de .

<msae/1b7b7741-c300-40f9-9c97-b5f129d45fb2> a skos:Concept ;
    skos:broader <msae/395c1569-a353-4cd5-bd95-3567528dc4a5> ;
    skos:inScheme <> ;
    skos:notation "1.1.1" ;
    skos:prefLabel "im Allgemeinen den Hauptpunkten von längeren Gesprächen folgen, die in ihrer Gegenwart geführt werden (B1),"@de .

<msae/1c19606d-6c5c-4c6b-bdda-a537010248b1> a skos:Concept ;
    skos:broader <msae/76a1c8c9-89ce-4bc9-89f4-dbffe578c0bd> ;
    skos:inScheme <> ;
    skos:notation "4.5.3" ;
    skos:prefLabel "Neue Technologien zur Informationsbeschaffung, zur kommunikativen Interaktion (E-Mail) und zur Präsentation der Ergebnisse nutzen."@de .

<msae/1e8fd703-68a1-43bd-b539-df04ead7a3b4> a skos:Concept ;
    skos:broader <msae/9e835887-30be-4231-9af1-2514af3850e5> ;
    skos:inScheme <> ;
    skos:notation "1.4.3" ;
    skos:prefLabel "für Ansichten, Pläne oder Handlungen kurze Begründungen oder Erklärungen geben (B1)."@de .

<msae/1f366a8b-9370-4ca1-9a77-03081363d01d> a skos:Concept ;
    skos:broader <msae/55b5b07b-753e-44bc-845f-38c86d25b15f> ;
    skos:inScheme <> ;
    skos:notation "1.5.5" ;
    skos:prefLabel "kurze einfache Aufsätze zu Themen von allgemeinem Interesse schreiben (B1),"@de .

<msae/274d00fd-dd90-4e7a-968b-ef6db7d2e3dc> a skos:Concept ;
    skos:broader <msae/55b5b07b-753e-44bc-845f-38c86d25b15f> ;
    skos:inScheme <> ;
    skos:notation "1.5.4" ;
    skos:prefLabel "unkomplizierte, detaillierte Texte zu einer Reihe verschiedener Themen aus ihren Interessengebieten verfassen, z. B. Erfahrungsberichte, Geschichten, Beschreibungen (B1),"@de .

<msae/277eeb07-7c13-4dbb-a2b7-3cc9da5424a5> a skos:Concept ;
    skos:broader <msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe> ;
    skos:inScheme <> ;
    skos:notation "3.1.5" ;
    skos:prefLabel "können sich in Bezug auf die Befindlichkeiten und Denkweisen in den fremdkulturellen Partner hineinversetzen,"@de .

<msae/27e47f70-7382-44ae-b96b-aa9d187ddadd> a skos:Concept ;
    skos:broader <msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe> ;
    skos:inScheme <> ;
    skos:notation "3.1.7" ;
    skos:prefLabel "können kulturelle Differenzen, Missverständnisse und Konfliktsituationen bewusst wahrnehmen, sich darüber verständigen und gegebenenfalls gemeinsam handeln."@de .

<msae/2da241b9-c5e8-432c-bf42-96f7c5e3a942> a skos:Concept ;
    skos:broader <msae/d65aa242-27cc-47c0-9776-12d30a056af3> ;
    skos:inScheme <> ;
    skos:notation "4.6.1" ;
    skos:prefLabel "den Nutzen der Fremdsprache zur Pflege von persönlichen und beruflichen Kontakten einschätzen,"@de .

<msae/2e4fbe68-5eed-4562-9f3e-9c4f9a3f4303> a skos:Concept ;
    skos:broader <msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d> ;
    skos:inScheme <> ;
    skos:notation "3.2.1" ;
    skos:prefLabel "das tägliche Leben (Alltag, Schule und Freizeit, Essen und Trinken, Arbeitszeiten und -gewohnheiten, Feiertage u.a.),"@de .

<msae/2ec32b6a-6c5f-4816-98f7-fe197728b31a> a skos:Concept ;
    skos:broader <msae/f9da45d2-7a32-4950-9197-78e65938b7b7> ;
    skos:inScheme <> ;
    skos:notation "1.2.1" ;
    skos:prefLabel "Korrespondenz lesen, die sich auf das eigene Interessengebiet bezieht und die wesentliche Aussage erfassen (B2),"@de .

<msae/2f1d9f1e-f97e-4e24-982e-56f26eda38c0> a skos:Concept ;
    skos:broader <msae/0113a01f-396d-4041-8c8f-90ce26e6534a> ;
    skos:inScheme <> ;
    skos:notation "4.2.3" ;
    skos:prefLabel "Techniken des Vermittelns zwischen zwei Sprachen einsetzen."@de .

<msae/2f561421-4245-452d-a24c-5ee632f0b702> a skos:Concept ;
    skos:broader <msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d> ;
    skos:inScheme <> ;
    skos:notation "3.2.3" ;
    skos:prefLabel "zwischenmenschliche Beziehungen (Geschlechterbeziehungen, Familienstrukturen, Generationsbeziehungen, u.a.),"@de .

<msae/2febadd0-c6fd-47de-8e8e-704f0a8b4696> a skos:Concept ;
    skos:broader <msae/908292d5-2247-4400-a1c2-c2c1f6f3f696> ;
    skos:inScheme <> ;
    skos:notation "2.2.4" ;
    skos:prefLabel "Handlungsperspektiven (aktive und passive Satzkonstruktionen) verstehen und selbst formulieren,"@de .

<msae/327a76f9-f21e-4d9b-8c25-3cc2bcb5bd47> a skos:Concept ;
    skos:broader <msae/f9da45d2-7a32-4950-9197-78e65938b7b7> ;
    skos:inScheme <> ;
    skos:notation "1.2.5" ;
    skos:prefLabel "die Aussagen einfacher literarischer Texte verstehen,"@de .

<msae/327c3ba8-58cd-4bce-b2ee-22580081bbf4> a skos:Concept ;
    skos:broader <msae/d65aa242-27cc-47c0-9776-12d30a056af3> ;
    skos:inScheme <> ;
    skos:notation "4.6.7" ;
    skos:prefLabel "Methoden des Spracherwerbs reflektieren und diese auf das Lernen weiterer Sprachen übertragen."@de .

<msae/399fccb2-a724-4838-abba-adf6dbbfb479> a skos:Concept ;
    skos:broader <msae/24b9ee01-f378-4d6b-95c1-a4698758813d> ;
    skos:inScheme <> ;
    skos:notation "2.3.1" ;
    skos:prefLabel "können verschiedenartige Aussprachevarianten der Zielsprache verstehen,"@de .

<msae/43e2e3b5-b7d2-4c92-82b4-aae72d36966e> a skos:Concept ;
    skos:broader <msae/6f0bd5e8-7220-4306-8a12-de9637a47e68> ;
    skos:inScheme <> ;
    skos:notation "4.1.1" ;
    skos:prefLabel "verschiedene Hör- und Lesetechniken auf unterschiedliche Textarten (z. B. Sachtexte, Artikel, literarische Kleinformen) anwenden,"@de .

<msae/48288907-fb19-4113-92ad-6be847ad4d19> a skos:Concept ;
    skos:broader <msae/cbe4d346-3bfb-4a8c-a139-d0d3291efffe> ;
    skos:inScheme <> ;
    skos:notation "3.1.1" ;
    skos:prefLabel "kennen elementare spezifische Kommunikations- und Interaktionsregeln ausgewählter englischsprachiger Länder und verfügen über ein entsprechendes Sprachregister, das sie in vertrauten Situationen anwenden können,"@de .

<msae/49687108-e732-46ea-a282-682eb55c441e> a skos:Concept ;
    skos:broader <msae/76a1c8c9-89ce-4bc9-89f4-dbffe578c0bd> ;
    skos:inScheme <> ;
    skos:notation "4.5.1" ;
    skos:prefLabel "Präsentationstechniken einsetzen (Medienwahl, Gliederungstechniken, Visualisierungstechniken, Gruppenpräsentation),"@de .

<msae/4995bf44-16b4-4887-95ad-ff10a35074df> a skos:Concept ;
    skos:broader <msae/9e02c338-d884-4787-9690-0cc45a194948> ;
    skos:inScheme <> ;
    skos:notation "1.3.6" ;
    skos:prefLabel "eine kurze Geschichte, einen Artikel, einen Vortrag, ein Interview oder eine Dokumentarsendung zu vertrauten Themen einem Gesprächspartner vorstellen und Informationsfragen dazu beantworten (B1+),"@de .

<msae/4d91f127-9d7a-429d-b1e0-449783af1145> a skos:Concept ;
    skos:broader <msae/395c1569-a353-4cd5-bd95-3567528dc4a5> ;
    skos:inScheme <> ;
    skos:notation "1.1.6" ;
    skos:prefLabel "das Wesentliche in vielen Fernsehsendungen zu Themen von persönlichem Interesse, z. B. Interviews, kurze Vorträge oder Nachrichtensendungen verstehen (B1+)."@de .

<msae/4eab9cb7-9e91-4b5b-bdf0-63677260cc34> a skos:Concept ;
    skos:broader <msae/f9da45d2-7a32-4950-9197-78e65938b7b7> ;
    skos:inScheme <> ;
    skos:notation "1.2.2" ;
    skos:prefLabel "klar formulierte Anweisungen, unkomplizierte Anleitungen, Hinweise und Vorschriften verstehen (B1/ B2),"@de .

<msae/510a5e80-66b0-4432-a237-eecf5624879c> a skos:Concept ;
    skos:broader <msae/f9da45d2-7a32-4950-9197-78e65938b7b7> ;
    skos:inScheme <> ;
    skos:notation "1.2.4" ;
    skos:prefLabel "in kürzeren literarischen Texten (z. B. Short Stories) die wesentlichen Aussagen erfassen und diese zusammentragen, um eine bestimmte Aufgabe zu lösen (B1),"@de .

<msae/62f100bc-526a-4020-a1e7-58fc4afd5525> a skos:Concept ;
    skos:broader <msae/908292d5-2247-4400-a1c2-c2c1f6f3f696> ;
    skos:inScheme <> ;
    skos:notation "2.2.2" ;
    skos:prefLabel "Handlungen, Ereignisse und Sachverhalte als gegenwärtig, vergangen, zukünftig oder zeitlos, mehrere Geschehnisse als gleichzeitig oder aufeinanderfolgend bzw. unter Berücksichtigung von Vor- und Nachzeitigkeit erkennen und wiedergeben,"@de .

<msae/64193303-50f9-4471-8056-578763fe3c94> a skos:Concept ;
    skos:broader <msae/41bd299d-d18a-4e72-a7eb-f6d9a12ffb8d> ;
    skos:inScheme <> ;
    skos:notation "3.2.4" ;
    skos:prefLabel "Werte, Normen, Überzeugungen, Einstellungen (in Bezug auf regionale Kulturen, Traditionen, Geschichte, Minderheiten, Kunst, u.a.)."@de .

<msae/6518a1c4-fdc5-4345-80a6-566c65b84cb7> a skos:Concept ;
    skos:broader <msae/f9da45d2-7a32-4950-9197-78e65938b7b7> ;
    skos:inScheme <> ;
    skos:notation "1.2.3" ;
    skos:prefLabel "längere Texte nach gewünschten Informationen durchsuchen und Informationen aus verschiedenen Texten zusammentragen, um eine bestimmte Aufgabe zu lösen (B1+),"@de .

<msae/66cb2b74-6ea9-4727-b4d5-73614b826875> a skos:Concept ;
    skos:broader <msae/908292d5-2247-4400-a1c2-c2c1f6f3f696> ;
    skos:inScheme <> ;
    skos:notation "2.2.3" ;
    skos:prefLabel "räumliche, zeitliche und logische Beziehungen erkennen und herstellen,"@de .

<msae/684a2aa5-7663-4130-bab8-d8617bfe912b> a skos:Concept ;
    skos:broader <msae/024c1435-9277-4c7f-a6a7-ea6c282bd672> ;
    skos:inScheme <> ;
    skos:notation "4.3.3" ;
    skos:prefLabel "mit Hilfe von Stichworten, Gliederungen, Handlungsgeländern Texte mündlich vortragen oder schriftlich verfassen,"@de .

<msae/6d7678be-c623-407b-a2b4-52abf2969387> a skos:Concept ;
    skos:broader <msae/908292d5-2247-4400-a1c2-c2c1f6f3f696> ;
    skos:inScheme <> ;
    skos:notation "2.2.5" ;
    skos:prefLabel "Anzahl, Art und Zugehörigkeit von Gegenständen, Lebewesen und Sachverhalten erkennen und beschreiben,"@de .

<msae/6e1c30fa-7d6b-4466-89d7-e7d2d06c13bc> a skos:Concept ;
    skos:broader <msae/9e02c338-d884-4787-9690-0cc45a194948> ;
    skos:inScheme <> ;
    skos:notation "1.3.2" ;
    skos:prefLabel "Gefühle wie Überraschung, Freude, Trauer, Interesse und Gleichgültigkeit ausdrücken und auf entsprechende Gefühlsäußerungen reagieren (B1),"@de .

<msae/70b1bc0b-4168-4d32-a5be-699d1b84403f> a skos:Concept ;
    skos:broader <msae/395c1569-a353-4cd5-bd95-3567528dc4a5> ;
    skos:inScheme <> ;
    skos:notation "1.1.3" ;
    skos:prefLabel "Ankündigungen und Mitteilungen zu konkreten Themen verstehen, die in normaler Geschwindigkeit in Standardsprache gesprochen werden (B2),"@de .

<msae/748776b8-cd8a-4197-8ada-7af601146d1a> a skos:Concept ;
    skos:broader <msae/24b9ee01-f378-4d6b-95c1-a4698758813d> ;
    skos:inScheme <> ;
    skos:notation "2.3.2" ;
    skos:prefLabel "beherrschen die Aussprache in der Weise, dass diese i.d.R. weder auf der Wort- noch auf der Satzebene zu Missverständnissen führt,"@de .

<msae/76c58d08-cf00-4213-9136-43fa0619223e> a skos:Concept ;
    skos:broader <msae/908292d5-2247-4400-a1c2-c2c1f6f3f696> ;
    skos:inScheme <> ;
    skos:notation "2.2.6" ;
    skos:prefLabel "Informationen wörtlich und vermittelt wiedergeben (direkte/indirekte Rede),"@de .

<msae/77dac0e2-d09c-4bc1-8496-2e3dfc938021> a skos:Concept ;
    skos:broader <msae/6f0bd5e8-7220-4306-8a12-de9637a47e68> ;
    skos:inScheme <> ;
    skos:notation "4.1.3" ;
    skos:prefLabel "wichtige Details durch Unterstreichen markieren,"@de .

<msae/77e7bfb4-3d43-4490-9b62-adf5b520bfc9> a skos:Concept ;
    skos:broader <msae/d65aa242-27cc-47c0-9776-12d30a056af3> ;
    skos:inScheme <> ;
    skos:notation "4.6.2" ;
    skos:prefLabel "selbstständig, mit einem Partner oder in Gruppen längere Zeit arbeiten,"@de .

<msae/79f8a6a9-a953-4827-b5a7-b1a8f1f1e189> a skos:Concept ;
    skos:broader <msae/9e02c338-d884-4787-9690-0cc45a194948> ;
    skos:inScheme <> ;
    skos:notation "1.3.4" ;
    skos:prefLabel "die meisten Dienstleistungsgespräche und routinemäßigen Situationen bewältigen, z. B. Umgang mit öffentlichen Einrichtungen während eines Auslandsaufenthaltes, Einkauf, Essen (B1),"@de .

<msae/7a15151d-992c-4471-90ce-122acb24f9fc> a skos:Concept ;
    skos:broader <msae/395c1569-a353-4cd5-bd95-3567528dc4a5> ;
    skos:inScheme <> ;
    skos:notation "1.1.5" ;
    skos:prefLabel "den Informationsgehalt der meisten Rundfunksendungen und Tonaufnahmen über Themen von persönlichem Interesse verstehen (B1+),"@de .

<msae/7a932ddf-93e1-49e7-9707-ba9033695b4a> a skos:Concept ;
    skos:broader <msae/9e02c338-d884-4787-9690-0cc45a194948> ;
    skos:inScheme <> ;
    skos:notation "1.3.1" ;
    skos:prefLabel "soziale Kontakte herstellen durch Begrüßung, Abschied, Sich-Vorstellen, Danken und Höflichkeitsformeln verwenden (A2),"@de .

<msae/7c434f12-9e68-4a6e-a3dc-cf37240241d0> a skos:Concept ;
    skos:broader <msae/908292d5-2247-4400-a1c2-c2c1f6f3f696> ;
    skos:inScheme <> ;
    skos:notation "2.2.1" ;
    skos:prefLabel "Aussagen, Fragen und Aufforderungen in bejahter und verneinter Form verstehen und formulieren,"@de .

<msae/7c9b486d-7456-434a-9164-aa4926ec92f8> a skos:Concept ;
    skos:broader <msae/55b5b07b-753e-44bc-845f-38c86d25b15f> ;
    skos:inScheme <> ;
