<msae/3fb227a6-723e-4e97-b2ad-3c9e4343b467> a skos:Concept ;
    skos:broader <msae/c6a1ad8a-5b01-4335-a362-50b153edc445> ;
    skos:inScheme <> ;
    skos:notation "3.2.3" ;
