<msae/2c1d0f8d-f1fd-480d-afe1-d918385ab4b0> a skos:Concept ;
    skos:broader <msae/3f61b18f-7a50-4db0-a35e-a7480df4ccbd> ;
    skos:inScheme <> ;
    skos:notation "1.2.1" ;
    skos:prefLabel "Korrespondenz lesen, die sich auf das eigene Interessengebiet bezieht und die wesentliche Aussage erfassen (B2),"@de .

<msae/2c63a3c7-abe6-4290-b40e-4a3d75540b76> a skos:Concept ;
    skos:broader <msae/17a0a144-2d6d-4e4a-b695-68005974b781> ;
    skos:inScheme <> ;
