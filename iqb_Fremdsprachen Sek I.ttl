    skos:inScheme <> ;
    skos:notation "1.1.1" ;
    skos:prefLabel "im Allgemeinen den Hauptpunkten von längeren Gesprächen folgen, die in ihrer Gegenwart geführt werden (B1),"@de .

<msae/4803faaa-f5f2-45d5-bc3c-fda4de6c51df> a skos:Concept ;
    skos:broader <msae/5ab32702-4457-4a33-b53c-1f7c18c28566> ;
    skos:inScheme <> ;
    skos:notation "2.3.2" ;
    skos:prefLabel "beherrschen die Aussprache in der Weise, dass diese i.d.R. weder auf der Wort- noch auf der Satzebene zu Missverständnissen führt,"@de .

<msae/49e4c920-7b8f-4995-81d0-177b1edfba62> a skos:Concept ;
    skos:broader <msae/82314bb0-68bb-4c41-b24d-4171224633ce> ;
    skos:inScheme <> ;
    skos:notation "4.4.2" ;
    skos:prefLabel "Verfahren zur Vernetzung, Strukturierung, Memorierung und Speicherung von sprachlichen Inputs, z. B. von Wortschatz, anwenden."@de .

<msae/4b5e4edf-c8b0-4931-8c55-02a7090e6fa8> a skos:Concept ;
    skos:broader <msae/ba311e65-fcdd-4a44-82fc-a4a5afb36273> ;
    skos:inScheme <> ;
    skos:notation "3.1.7" ;
    skos:prefLabel "können kulturelle Differenzen, Missverständnisse und Konfliktsituationen bewusst wahrnehmen, sich darüber verständigen und gegebenenfalls gemeinsam handeln."@de .

<msae/4f0c5299-1ed7-41f9-af37-81982e2bd2cc> a skos:Concept ;
    skos:broader <msae/53fec671-c684-4869-b56b-fc892aa3cbc0> ;
    skos:inScheme <> ;
    skos:notation "4.5.1" ;
    skos:prefLabel "Präsentationstechniken einsetzen (Medienwahl, Gliederungstechniken, Visualisierungstechniken, Gruppenpräsentation),"@de .

<msae/5b5dbfb5-b30c-44a6-bfdb-e54eb6c82556> a skos:Concept ;
    skos:broader <msae/2b91e0fb-4ca0-4608-a920-77c44c18664a> ;
    skos:inScheme <> ;
    skos:notation "2.2.3" ;
    skos:prefLabel "räumliche, zeitliche und logische Beziehungen erkennen und herstellen,"@de .

<msae/5c784c5d-4759-42f8-9254-086734724737> a skos:Concept ;
    skos:broader <msae/ba311e65-fcdd-4a44-82fc-a4a5afb36273> ;
    skos:inScheme <> ;
    skos:notation "3.1.6" ;
