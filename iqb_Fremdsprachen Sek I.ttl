        "https://example.com/iqb/bista2004ef1/msaf/4/4.5",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6" .

<msaf/4/4.1> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4" ;
    skos:prefLabel "Textrezeption (Leseverstehen und Hörverstehen)" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msaf/4/4.1/4.1.1",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.1/4.1.2",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.1/4.1.3",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.1/4.1.4" .

<msaf/4/4.1/4.1.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.1" ;
    skos:prefLabel "verschiedene Hör- und Lesetechniken auf unterschiedliche Textarten (z. B. Sachtexte, Artikel, literarische Kleinformen) anwenden," .

<msaf/4/4.1/4.1.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.1" ;
    skos:prefLabel "sich schnell einen groben Überblick über den Inhalt eines Textes verschaffen," .

<msaf/4/4.1/4.1.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.1" ;
    skos:prefLabel "wichtige Details durch Unterstreichen markieren," .

<msaf/4/4.1/4.1.4> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.1" ;
    skos:prefLabel "wichtige Textstellen durch farbliches Hervorheben, durch das Notieren von Stichworten und durch ordnende ergänzende Randnotizen besonders kenntlich machen." .

<msaf/4/4.2> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4" ;
    skos:prefLabel "Interaktion" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msaf/4/4.2/4.2.1",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.2/4.2.2",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.2/4.2.3" .

<msaf/4/4.2/4.2.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.2" ;
    skos:prefLabel "sich in der Klasse, mit Partnern und in Gruppen in der Fremdsprache verständigen und Kommunikationsprozesse aufrecht erhalten," .

<msaf/4/4.2/4.2.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.2" ;
    skos:prefLabel "sich in realen Alltagssituationen mit unterschiedlichen, französischsprachigen Personen verständigen und Verständigungsprobleme durch Rückfragen, durch Vereinfachungen, durch Höflichkeitsformeln und nonverbale Mittel überwinden," .

<msaf/4/4.2/4.2.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.2" ;
    skos:prefLabel "Techniken des Vermittelns zwischen zwei Sprachen einsetzen." .

<msaf/4/4.3> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4" ;
    skos:prefLabel "Textproduktion (Sprechen und Schreiben)" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msaf/4/4.3/4.3.1",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.3/4.3.2",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.3/4.3.3",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.3/4.3.4" .

<msaf/4/4.3/4.3.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.3" ;
    skos:prefLabel "sich Informationen aus unterschiedlichen fremdsprachlichen Textquellen beschaffen, die se vergleichen, auswählen und bearbeiten," .

<msaf/4/4.3/4.3.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.3" ;
    skos:prefLabel "Techniken des Notierens zur Vorbereitung eigener Texte oder Präsentationen nutzen," .

<msaf/4/4.3/4.3.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.3" ;
    skos:prefLabel "mit Hilfe von Stichworten, Gliederungen, Handlungsgeländern Texte mündlich vortragen oder schriftlich verfassen," .

<msaf/4/4.3/4.3.4> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.3" ;
    skos:prefLabel "die Phasen des Schreibprozesses (Entwerfen, Schreiben, Überarbeiten) selbstständig durchführen." .

<msaf/4/4.4> skos:definition "Lernstrategien beziehen sich auf sprachliche Mittel, auf Aussprache, Intonation, Orthographie sowie auf Grammatik. Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4" ;
    skos:prefLabel "Lernstrategien" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msaf/4/4.4/4.4.1",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.4/4.4.2" .

<msaf/4/4.4/4.4.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.4" ;
    skos:prefLabel "Hilfsmittel zum Nachschlagen und Lernen, z. B. Wörterbücher, Grammatikbücher usw. selbstständig nutzen," .

<msaf/4/4.4/4.4.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.4" ;
    skos:prefLabel "Verfahren zur Vernetzung, Strukturierung, Memorierung und Speicherung von sprachlichen Inputs, z. B. von Wortschatz, anwenden." .

<msaf/4/4.5> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4" ;
    skos:prefLabel "Präsentation und Medien" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msaf/4/4.5/4.5.1",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.5/4.5.2",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.5/4.5.3" .

<msaf/4/4.5/4.5.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.5" ;
    skos:prefLabel "Präsentationstechniken einsetzen (Medienwahl, Gliederungstechniken, Visualisierungstechniken, Gruppenpräsentation)," .

<msaf/4/4.5/4.5.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.5" ;
    skos:prefLabel "mit Lernprogrammen (auch Multimedia) arbeiten," .

<msaf/4/4.5/4.5.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.5" ;
    skos:prefLabel "Neue Technologien zur Informationsbeschaffung, zur kommunikativen Interaktion (E-Mail) und zur Präsentation der Ergebnisse nutzen." .

<msaf/4/4.6> skos:definition "Die Schülerinnen und Schüler können" ;
    skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4" ;
    skos:prefLabel "Lernbewusstheit und Lernorganisation" ;
    skos:topConceptOf "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.1",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.2",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.3",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.4",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.5",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.6",
        "https://example.com/iqb/bista2004ef1/msaf/4/4.6/4.6.7" .

<msaf/4/4.6/4.6.1> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "den Nutzen der Fremdsprache zur Pflege von persönlichen und beruflichen Kontakten einschätzen," .

<msaf/4/4.6/4.6.2> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "selbstständig, mit einem Partner oder in Gruppen längere Zeit arbeiten," .

<msaf/4/4.6/4.6.3> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "ausgewählte Projekte (z. B. bilinguale Projekte) bearbeiten," .

<msaf/4/4.6/4.6.4> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "für sie förderliche Lernbedingungen erkennen und nutzen, ihre Lernarbeit organisieren und die Zeit einteilen," .

<msaf/4/4.6/4.6.5> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "Fehler erkennen und diese Erkenntnisse für den eigenen Lernprozess nutzen," .

<msaf/4/4.6/4.6.6> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "ihren eigenen Lernfortschritt beschreiben und ggf. in einem Portfolio dokumentieren," .

<msaf/4/4.6/4.6.7> skos:hasTopConcept "https://example.com/iqb/bista2004ef1/msaf/4/4.6" ;
    skos:prefLabel "Methoden des Spracherwerbs reflektieren und diese auf das Lernen weiterer Sprachen übertragen." .

